{
  "name": "image-processing",
  "version": "1.0.0",
  "description": "Electron-based desktop app providing an interface to wrap around Kelly Clark & Melissa Martin's neuroimaging pre-processing pipeline, available on [docker hub](https://hub.docker.com/r/terf/image-processing).",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "dist": "./node_modules/.bin/electron-builder --publish never -wl",
    "dist-mac": "./node_modules/.bin/electron-builder -m"
  },
  "build": {
    "win": {
      "target": "NSIS"
    },
    "linux": {
      "target": "deb"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Terf/mri-processing.git"
  },
  "author": "Tim Robert-Fitzgerald <Timothy.Robert-Fitzgerald@Pennmedicine.upenn.edu>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Terf/mri-processing/issues"
  },
  "homepage": "https://github.com/Terf/mri-processing#readme",
  "devDependencies": {
    "electron": "^8.1.1",
    "electron-builder": "^22.4.1"
  },
  "dependencies": {
    "fix-path": "^3.0.0"
  }
}
